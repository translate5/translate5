<?xml version="1.0" encoding="UTF-8" ?>
<testcase name="ESP® Kurvendynamik-Assistent and tag included" creator="beo/MittagQI" mandatory="no" sourceLang="DE" targetLang="EN" tbxPath="beo-reallive-DE-EN.tbx" sourceStringMatch="0" targetStringMatch="0">
