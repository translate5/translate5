      <trans-unit id="01" ax:state-name="Corrected" ax:state-id="4" ax:state-flags="37748736">
        <source><bpt id="1" rid="1" ax:s-id="11" ax:s-is-base-style="True" ax:s-font-name="Myriad Pro Cond" ax:s-font-size="7.5" ax:s-fore-color="-16777216" ax:s-script="Baseline" ax:s-caps="Normal" /><bpt id="3" rid="2" ax:s-id="3" />Thank you for choosing a <ph id="5" ax:element-id="6000">COMPANY</ph> blood pressure monitor.<ept id="4" rid="2" /><ept id="2" rid="1" /></source>
        <target><bpt id="1" rid="1" ax:s-id="11" ax:s-is-base-style="True" ax:s-font-name="Myriad Pro Cond" ax:s-font-size="7.5" ax:s-fore-color="-16777216" ax:s-script="Baseline" ax:s-caps="Normal" /><bpt id="3" rid="2" ax:s-id="3" />Zahvaljujemo se vam, ker ste izbrali meilnik krvnega tlaka <ph id="5" ax:element-id="6000">COMPANY</ph>.<ept id="4" rid="2" /><ept id="2" rid="1" /></target>
