<!-- Don't split at e.g. "U. S. A." -->
<rule break="no">
<beforebreak>[^-\p{L}]\p{L}[\.!?…]['|"|«|\)|\]|\}]?\s</beforebreak>
<afterbreak></afterbreak>
</rule>
